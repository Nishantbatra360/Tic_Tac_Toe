<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nishant.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TicTacToe">
        <activity
            android:name=".Model.HighScoreActivity"
            android:exported="true" />
        <activity
            android:name=".Model.MainActivity"
            android:exported="true" />
        <activity
            android:name=".CustomDialogs.MultiPlayerInformationDialogActivity"
            android:exported="true" />
        <activity
            android:name=".Model.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Model.PlayActivity"
            android:exported="true" />
        <service android:name=".Services.TimeService"/>
    </application>

</manifest>